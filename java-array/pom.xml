<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">

  <modelVersion>4.0.0</modelVersion>

  <groupId>fr.imj</groupId>
  <artifactId>siren-java-array</artifactId>
  <name>siren-java-array</name>
  <version>1.0.0-SNAPSHOT</version>
  <packaging>jar</packaging>
 
  <build>
    <plugins>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-compiler-plugin</artifactId>
        <version>3.3</version>
        <configuration>
          <source>1.7</source>
          <target>1.7</target>
        </configuration>
      </plugin>
    </plugins>
  </build> 

  <properties>
    <responsible>Aur√©lien Larive</responsible>
    <responsibleMail>alarive@imj.fr</responsibleMail>
    <maven.build.timestamp.format>dd/MM/yy HH:mm</maven.build.timestamp.format>
    <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
  </properties>
</project>
